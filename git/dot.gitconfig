[user]
  name  = Scott Davis
  email = scott@thirstyhead.com

[alias]
  b  = branch
  c  = commit
  cm = commit -m 
  co = checkout
  s  = status
  new = !git init && git symbolic-ref HEAD refs/heads/main

[core]
  excludesfile = /Users/scott/.gitexcludes
  autocrlf = input

[branch]
  autosetupmerge = true

[push]
  default = current

[color]
  ui = auto

[init]
  defaultBranch = main
